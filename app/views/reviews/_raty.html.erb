
<div class="movie-evaluation-<%= review.id %>"></div>
<script>
  $(".movie-evaluation-<%= review.id %>").empty();
  $('.movie-evaluation-<%= review.id %>').raty({
    size: 36,
    starOff:  '<%= asset_path('star-off.png') %>',
    starOn : '<%= asset_path('star-on.png') %>',
    starHalf: '<%= asset_path('star-half.png') %>',
    readOnly: true,
    score: <%= review.rate %>,
  });
</script>
              